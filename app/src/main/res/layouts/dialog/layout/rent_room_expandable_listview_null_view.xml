<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/white"
    android:columnCount="4"
    android:gravity="start"
    android:layoutDirection="ltr">

    <my_manage.tool.IconView
        android:id="@+id/rent_room_expandable_listview_null_details"
        style="@style/rental_expandable_listview_item"
        android:text="&#xe66f;\n查看详情" />

    <my_manage.tool.IconView
        android:id="@+id/rent_room_expandable_listview_null_rent"
        style="@style/rental_expandable_listview_item"
        android:text="&#xe63d;\n出租" />

    <my_manage.tool.IconView
        android:id="@+id/rent_room_expandable_listview_null_leaseback"
        style="@style/rental_expandable_listview_item"
        android:text="&#xe676;\n撤销退租" />


    <my_manage.tool.IconView
        android:id="@+id/rent_room_expandable_listview_null_history"
        style="@style/rental_expandable_listview_item"
        android:text="&#xe650;\n查看出租记录" />

    <my_manage.tool.IconView
        android:id="@+id/rent_room_expandable_listview_null_del_room"
        style="@style/rental_expandable_listview_item"
        android:layout_marginBottom="@dimen/rental_main_gridview_margin_6"
        android:text="&#xe63d;\n删除房源" />
</GridLayout>