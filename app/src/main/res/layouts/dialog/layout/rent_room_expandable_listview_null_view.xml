<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="?android:attr/listPreferredItemHeight"
    android:background="@android:color/white"
    android:columnCount="4"
    android:gravity="start"
    android:layoutDirection="ltr">

    <TextView
        android:id="@+id/rent_room_expandable_listview_null_details"
        style="@style/rental_expandable_listview_item"
        android:text="查看详情" />

    <TextView
        android:id="@+id/rent_room_expandable_listview_null_rent"
        style="@style/rental_expandable_listview_item"
        android:text="出租" />

    <TextView
        android:id="@+id/rent_room_expandable_listview_null_leaseback"
        style="@style/rental_expandable_listview_item"
        android:text="撤销退租" />


    <TextView
        android:id="@+id/rent_room_expandable_listview_null_history"
        style="@style/rental_expandable_listview_item"
        android:text="查看出租记录" />
</GridLayout>