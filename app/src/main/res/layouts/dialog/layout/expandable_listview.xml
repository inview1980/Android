    android:id="@+id/main_viewId"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context=".page.PasswordManageActivity">



    <ImageButton
        android:id="@+id/main_expandable_add_btn"
