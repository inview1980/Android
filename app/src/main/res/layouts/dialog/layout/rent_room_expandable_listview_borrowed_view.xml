<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/rental_main_gridview_margin_6"
    android:background="@color/grayWhite2"
    android:columnCount="4"
    android:gravity="start"
    android:layoutDirection="ltr">

    <my_manage.tool.IconView
        android:id="@+id/rent_room_expandable_listview_borrowed_details"
        style="@style/rental_expandable_listview_item"
        android:layout_marginTop="@dimen/rental_main_gridview_margin_9"
        android:layout_marginBottom="@dimen/rental_main_gridview_margin_6"
        android:text="&#xe66f;\n查看详情" />

    <my_manage.tool.IconView
        android:id="@+id/rent_room_expandable_listview_continue_rent"
        style="@style/rental_expandable_listview_item"
        android:text="&#xe666;\n续租" />

    <my_manage.tool.IconView
        android:id="@+id/rent_room_expandable_listview_borrowed_leaseback"
        style="@style/rental_expandable_listview_item"
        android:text="&#xe61b;\n退租" />

    <my_manage.tool.IconView
        android:id="@+id/rent_room_expandable_listview_borrowed_changed_rent"
        style="@style/rental_expandable_listview_item"
        android:text="&#xe676;\n调整租金" />

    <my_manage.tool.IconView
        android:id="@+id/rent_room_expandable_listview_borrowed_changed_deposit"
        style="@style/rental_expandable_listview_item"
        android:layout_marginTop="@dimen/rental_main_gridview_margin_6"
        android:layout_marginBottom="@dimen/rental_main_gridview_margin_6"
        android:text="&#xe667;\n调整押金" />

    <my_manage.tool.IconView
        android:id="@+id/rent_room_expandable_listview_borrowed_history"
        style="@style/rental_expandable_listview_item"
        android:layout_marginTop="@dimen/rental_main_gridview_margin_6"
        android:layout_marginBottom="@dimen/rental_main_gridview_margin_6"
        android:text="&#xe650;\n查看出租记录" />

    <my_manage.tool.IconView
        android:id="@+id/rent_room_expandable_listview_borrowed_pay_propertycosts"
        style="@style/rental_expandable_listview_item"
        android:layout_columnWeight="@integer/rental_room_details_1width"
        android:layout_marginTop="@dimen/rental_main_gridview_margin_6"
        android:layout_marginBottom="@dimen/rental_main_gridview_margin_6"
        android:text="&#xe60e;\n支付物业费" />

    <my_manage.tool.IconView
        android:id="@+id/rent_room_expandable_listview_borrowed_renew_contract"
        style="@style/rental_expandable_listview_item"
        android:layout_marginTop="@dimen/rental_main_gridview_margin_6"
        android:layout_marginBottom="@dimen/rental_main_gridview_margin_6"
        android:text="&#xe616;\n续签合同" />

    <my_manage.tool.IconView
        android:id="@+id/rent_room_expandable_listview_borrowed_man_info"
        style="@style/rental_expandable_listview_item"
        android:layout_marginTop="@dimen/rental_main_gridview_margin_6"
        android:layout_marginBottom="@dimen/rental_main_gridview_margin_9"
        android:text="&#xe611;\n租户资料" />

    <my_manage.tool.IconView
        android:id="@+id/rent_room_expandable_listview_borrowed_del_room"
        style="@style/rental_expandable_listview_item"
        android:layout_marginBottom="@dimen/rental_main_gridview_margin_6"
        android:text="&#xe63d;\n删除房源" />
</GridLayout>